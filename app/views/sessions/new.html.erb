<h1>Login</h1>
<p id="alert"><%= alert %></p>
<%= bootstrap_form_tag url: '/sessions' do |f| %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="actions"><%= submit_tag "Log in" %></div>
<% end %>
